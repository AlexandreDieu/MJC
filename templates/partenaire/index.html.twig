{% extends 'base.html.twig' %}

{% block title %}Partenaire index{% endblock %}

{% block body %}
    <h1>Partenaire index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NomStructure</th>
                <th>StatutJuridique</th>
                <th>NomResponsable</th>
                <th>PrenomResponsable</th>
                <th>NomContact</th>
                <th>PrenomContact</th>
                <th>StatutPartenaire</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for partenaire in partenaires %}
            <tr>
                <td>{{ partenaire.id }}</td>
                <td>{{ partenaire.nomStructure }}</td>
                <td>{{ partenaire.statutJuridique }}</td>
                <td>{{ partenaire.nomResponsable }}</td>
                <td>{{ partenaire.prenomResponsable }}</td>
                <td>{{ partenaire.nomContact }}</td>
                <td>{{ partenaire.prenomContact }}</td>
                <td>{{ partenaire.statutPartenaire }}</td>
                <td>
                    <a href="{{ path('partenaire_show', {'id': partenaire.id}) }}">show</a>
                    <a href="{{ path('partenaire_edit', {'id': partenaire.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('partenaire_new') }}">Create new</a>
{% endblock %}
